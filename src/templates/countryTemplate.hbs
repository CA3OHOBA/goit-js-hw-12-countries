{{#each this}}
  <div class='country-div'>
    <img src={{flag}} width="350" alt='flag of {{name}}' />
    <h2>Name of Country: {{name}}</h2>
    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    <ul>
      {{#each languages}}
        <li>{{this.name}}</li>
      {{/each}}
    </ul>
  </div>
{{/each}}